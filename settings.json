{
   /* Color Theme */
   "workbench.colorTheme": "Dark Glass",
   /* Font selection */
   "editor.fontFamily": "'Cartograph'",
   "terminal.integrated.fontFamily": "'Cartograph'",
   "editor.fontVariations": true,
   "editor.fontLigatures": "'calt', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09', 'liga'",
   "editor.fontWeight": "400",
   /* Theme Related Settings */
  "editor.lineHeight": 1.8,
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "always",
  "editor.minimap.showSlider": "always",
  "workbench.iconTheme": "vs-seti-folder"
}
